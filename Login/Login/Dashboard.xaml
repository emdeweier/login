<UserControl x:Class="Login.Dashboard"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Login"
             mc:Ignorable="d" 
             d:DesignHeight="400" d:DesignWidth="600">
    <Grid Background="#FFBDCEC4">
        <Rectangle Fill="#FF2a2b20" HorizontalAlignment="Center" Height="50" Margin="0,0,0,0" VerticalAlignment="Top" Width="600" Stroke="#FF2A2b20"/>
        <Rectangle Fill="#FF9EC5A3" HorizontalAlignment="Left" Height="350" Margin="0,50,0,0" VerticalAlignment="Top" Width="170" Stroke="#FF9EC5A3"/>
        <Button Content="Profile" HorizontalAlignment="Left" Margin="30,150,0,0" VerticalAlignment="Top" Width="105" Height="25" Background="#FF3B3C30" Foreground="#FFBDCEC4" BorderBrush="#FF3B3C30"/>
        <Button Content="Insert Data" HorizontalAlignment="Left" Margin="30,180,0,0" VerticalAlignment="Top" Width="105" Height="25" Background="#FF3B3C30" Foreground="#FFBDCEC4" BorderBrush="#FF3B3C30"/>
        <Button Content="View Data" HorizontalAlignment="Left" Margin="30,210,0,0" VerticalAlignment="Top" Width="105" Height="25" Background="#FF3B3C30" Foreground="#FFBDCEC4" BorderBrush="#FF3B3C30"/>
    </Grid>
</UserControl>
